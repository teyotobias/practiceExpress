<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>First Express</title>
</head>
<body>
    <h1>All Students</h1>
    <ul>
      <% students.forEach(function(st) { %>
        <li>
          <%= st.name %>
            - 
          <%= st.grade == 'NA' ? 'graduated' : st.grade + 'th' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>